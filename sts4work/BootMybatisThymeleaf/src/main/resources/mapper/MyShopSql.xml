<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myshop.mapper.MyShopMapper">
	<select id="getTotalCount" resultType="int">
		select count(*) from myshop
	</select>
	<insert id="insertShop" parameterType="ShopDto">
		insert into myshop values(null,#{sangpum},#{price},#{photo},#{color},now())
	</insert>
	<select id="getData" parameterType="int" resultType="ShopDto">
		select * from myshop where num=#{num}
	</select>
	<delete id="deleteShop" parameterType="int">
		delete from myshop where num=#{num}
	</delete>
</mapper>